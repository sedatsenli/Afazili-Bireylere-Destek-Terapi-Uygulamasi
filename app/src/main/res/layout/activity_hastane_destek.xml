<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient"
    tools:context=".hastane_destek">

    <androidx.gridlayout.widget.GridLayout
        android:id="@+id/gridLayout"
        android:layout_width="359dp"
        android:layout_height="342dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/susadim"
            android:layout_width="163dp"
            android:layout_height="wrap_content"
            android:background="@drawable/oval_btn2"
            android:backgroundTint="#2859C5"
            android:backgroundTintMode="src_over"
            android:hapticFeedbackEnabled="false"
            android:onClick="susadim"
            android:text="SUSADIM"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textStyle="bold"
            app:iconTint="#721212"
            app:layout_column="2"
            app:layout_row="1"
            app:rippleColor="#902828" />

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="161dp"
            android:layout_height="110dp"
            app:layout_column="2"
            app:layout_row="0"
            app:srcCompat="@drawable/susadim" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/aciktim"
            android:layout_width="166dp"
            android:layout_height="wrap_content"
            android:background="@drawable/oval_btn2"
            android:backgroundTint="#2859C5"
            android:backgroundTintMode="src_over"
            android:hapticFeedbackEnabled="false"
            android:onClick="aciktim"
            android:text="ACIKTIM"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textStyle="bold"
            app:iconTint="#721212"
            app:layout_column="0"
            app:layout_row="1"
            app:rippleColor="#902828" />

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="163dp"
            android:layout_height="111dp"
            app:layout_column="2"
            app:layout_row="3"
            app:srcCompat="@drawable/tuvalet" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="167dp"
            android:layout_height="108dp"
            app:layout_column="0"
            app:layout_row="0"
            app:srcCompat="@drawable/aciktim" />

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="167dp"
            android:layout_height="114dp"
            app:layout_column="0"
            app:layout_row="3"
            app:srcCompat="@drawable/cagri" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/tuvaletIhtiyaci"
            android:layout_width="163dp"
            android:layout_height="wrap_content"
            android:background="@drawable/oval_btn2"
            android:backgroundTint="#2859C5"
            android:backgroundTintMode="src_over"
            android:hapticFeedbackEnabled="false"
            android:onClick="tuvaletIhtiyaci"
            android:text="TUVALET İHTİYACI"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textStyle="bold"
            app:iconTint="#721212"
            app:layout_column="2"
            app:layout_row="4"
            app:rippleColor="#902828" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/aileArama"
            android:layout_width="166dp"
            android:layout_height="wrap_content"
            android:background="@drawable/oval_btn2"
            android:backgroundTint="#2859C5"
            android:backgroundTintMode="src_over"
            android:hapticFeedbackEnabled="false"
            android:onClick="aileArama"
            android:text="AİLEMİ ARA"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textStyle="bold"
            app:iconTint="#721212"
            app:layout_column="0"
            app:layout_row="4"
            app:rippleColor="#902828" />

    </androidx.gridlayout.widget.GridLayout>

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="top"
        tools:layout_editor_absoluteY="731dp" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</androidx.constraintlayout.widget.ConstraintLayout>